{   
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-process-exit": "off",
        "class-methods-use-this": "off"
    },
    "overrides": [{
        "files": "**/*.test.js",
        "rules": {
            "node/no-unpublished-require": 0,
            "node/no-missing-require": 0
        }
    }]
    
}